@model CoreShoppingAdminPortal.Models.Feedback
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h3>Are you sure you want to delete ? </h3>
&nbsp; &nbsp; &nbsp;
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">@Html.DisplayNameFor(Model => Model.FeedbackId)</th>
            <th scope="col">@Html.DisplayNameFor(Model => Model.EmailId)</th>
            <th scope="col">@Html.DisplayNameFor(Model => Model.Comment)</th>
            <th scope="col">@Html.DisplayNameFor(Model => Model.PhoneNo)</th>
            <th scope="col">@Html.DisplayNameFor(Model => Model.CustomerId)</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Html.DisplayFor(Model => Model.FeedbackId)</td>
            <td>@Html.DisplayFor(Model => Model.EmailId)</td>
            <td>@Html.DisplayFor(Model => Model.Comment)</td>
            <td>@Html.DisplayFor(Model => Model.PhoneNo)</td>
            <td>@Html.DisplayFor(Model => Model.CustomerId)</td>
        </tr>

    </tbody>
</table>
<br />
@using (Html.BeginForm("Delete", "Feedback", FormMethod.Post))
{
    <center>
        <input type="submit" value="Delete" class="btn btn-success" />&nbsp;
        <button class="btn btn-success" asp-controller="Feedback" asp-action="Index">
            Back
        </button>
    </center>
}