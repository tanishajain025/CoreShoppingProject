@model CoreEcommerceUserPanal.Models.Feedbacks
@{
    ViewData["Title"] = "Feedback";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}
<style>


    .im1 {
        margin-left: 300px;
        margin-top: -400px;
    }
</style>
<!-- Start Banner Area -->
<section class="banner-area organic-breadcrumb">
    <div class="container">
        <div class="breadcrumb-banner d-flex flex-wrap align-items-center justify-content-end">
            <div class="col-first">
                <h1>Feedback</h1>

            </div>
        </div>
    </div>
</section>

<div class="container">
    <div class="row">
        <div class="col-xl-3 col-lg-4 col-md-5">
            <div class="sidebar-categories">
                <br /><br />
                <ul class="main-categories">
                    <li class="main-nav-list">
                        <a data-toggle="collapse" href="#fruitsVegetable" aria-expanded="false" aria-controls="fruitsVegetable">
                            <span class="lnr lnr-arrow-right"></span> <a class="nav-link" asp-controller="customers" asp-action="Details">My Profile</a>
                        </a>

                    </li>
                    <li class="main-nav-list">
                        <a data-toggle="collapse" href="#meatFish" aria-expanded="false" aria-controls="meatFish">
                            <span class="lnr lnr-arrow-right"></span> <a class="nav-link" asp-controller="customers" asp-action="OrderHistory">My Orders</a>
                        </a>

                    </li>
                    <li class="main-nav-list">
                        <a data-toggle="collapse" href="#cooking" aria-expanded="false" aria-controls="cooking">
                            <span class="lnr lnr-arrow-right"></span> <a class="nav-link" asp-controller="customers" asp-action="custEdit">Edit Profile</a>
                        </a>

                    </li>
                    <li class="main-nav-list">
                        <a data-toggle="collapse" href="#beverages" aria-expanded="false" aria-controls="beverages">
                            <span class="lnr lnr-arrow-right"></span><a class="nav-link" asp-controller="customers" asp-action="password">Reset Password</a>
                        </a>

                    </li>
                    <li class="main-nav-list">
                        <a data-toggle="collapse" href="#homeClean" aria-expanded="false" aria-controls="homeClean">
                            <span class="lnr lnr-arrow-right"></span> <a class="nav-link" asp-controller="customers" asp-action="Feedback">Feedback</a>
                        </a>

                    </li>
                    <br />
                    <br />
                </ul>
            </div>
        </div>
    </div>
</div>

<br /><br />

<body>
    <form class="contact1-form validate-form" asp-action="Feedback" asp-controller="Customers">
        <div class="col-md-6 im1">
            <div class="form-group">
                <label>Username</label>
                <input type="text" class="form-control" asp-items="ViewBag.feed" value="@ViewBag.cusname" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter your name'">
            </div>
            <div class="form-group">
                <label asp-for="EmailId">Email Id</label>
                <input type="email" asp-for="EmailId" class="form-control" id="email" placeholder="Enter email address" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter email address'">
            </div>
            <div class="form-group">
                <label asp-for="PhoneNo">Phone No</label>
                <input class="form-control" asp-for="PhoneNo" type="text" id="phone" placeholder="Phone" required>
            </div>
          
                <div class="form-group">
                    <label asp-for="Comment">Message</label>
                    <textarea class="form-control" asp-for="Comment" id="message" rows="1" placeholder="Enter Message" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter Message'" required></textarea>
                </div>
           
            <div class="col-md-12 text-left">
                  
                <button type="submit" value="submit" class="primary-btn" onclick="myFunction()">Send </button>
            </div>
        </div>
     
    </form>

    <br />

    <script>
        function myFunction() {
            alert("Feedback Successfully Submitted !!!");
        }
    </script>



    <script>
        $('.js-tilt').tilt({
            scale: 1.1
        })
    </script>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-23581568-13"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-23581568-13');
    </script>

    <!--===============================================================================================-->
    <script src="~/js/main.js"></script>

</body>
